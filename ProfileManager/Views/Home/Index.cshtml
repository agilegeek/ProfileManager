@model ProfileManager.ViewModels.HomeViewModel

<h2>@Model.Title</h2>

@foreach(var applicatie in Model.Applicaties)
{
<div class="col-sm-4 col-lg-4 col-md-4">
    <div class="thumbnail">
        
        <div class="caption">
            <h3 class="pull-right">@applicatie.Versie</h3>
            <h3>
                @applicatie.Naam
            </h3>
            <p>@applicatie.Technologie</p>
        </div>
    </div>
</div>
}
